  "scripts": {
    "clean": "gulp clean",
    "dev": "gulp dev",
    "prod": "gulp prod",
    "vsass": "gulp vbuild",
    "build_normal": "webpack --colors --progress --mode=build_normal",
    "build_min": "webpack --colors --progress --mode=build_min",
    "neoui-build": "npm run build_normal && npm run build_min",
    "neoui-dev": "webpack --progress --colors  --mode=product_normal && webpack --progress --colors  --mode=product_min",
    "plugin": "npm run neoui-build && gulp neoplugin && node concat",
    "product": "npm run neoui-dev && gulp cssvs",
    "es": "babel js -d lib"
  },
